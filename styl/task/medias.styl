@media screen and (min-width 2295px)
	body
		font-size: 20px
	.container
		width 1400px
@media screen and (max-width 992px)
	header
		padding 15px 0
	#logo
		float: none
		margin 0 auto 5px
		display block
	.contacts
		text-align: center
		float: none
	.contact
		display: inline-block
		margin 5px 5px 0
@media screen and (max-width 485px)
	header
		padding: 10px 0
	#logo
		width 160px
		margin-bottom: 0
	.contacts
		line-height: 15px
	.contact
		font-size: 14px
	#main
		padding 40px 0 15px
	#feedback
		padding 40px 0
	.main-title
		font-size: 24px
	.general-title
		font-size: 22px
		margin-bottom: 25px
	footer 
		padding 15px 0
		font-size: 12px
		h5
			font-size: 18px
	.soc-buttons a
		margin 10px 5px
@media screen and (max-width 385px)
	#main
		padding 30px 0 5px
	#feedback
		padding 30px 0
	.main-title
		font-size: 22px
	.general-title
		font-size: 20px
		margin-bottom: 25px