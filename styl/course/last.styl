#last
	padding 60px 0
	background-color: #E9E9E9
	overflow hidden
.last-container
	text-align: center
	padding 70px 40px
	border: 3px solid #000000
	border-radius: 25px
	position relative
	&:before
		content: ''
		display block
		position: absolute
		width 80px
		height 77px
		background-image url(../img/last_icon_1.jpg)
		background-repeat no-repeat
		background-position: center
		background-size contain
		top -30px
		left -30px
	&:after
		content: ''
		display block
		position: absolute
		width 151px
		height 115px
		background-image url(../img/last_icon_2.jpg)
		background-repeat no-repeat
		background-position: center
		background-size contain
		bottom -45px
		right -45px
.last-container-title
	font-size: 34px;
	font-family: "RussoOne", sans-serif;
	margin-bottom: 40px
.button-last
	background-color: #00AE5D
	width 100%
	max-width 413px
	line-height 70px
	margin 0 auto
	box-shadow: 0 0 0 17px #E9E9E9
	transition: all .3s ease