#changes
#for
	padding 60px 0
	background-color: #E9E9E9
#for
	padding-bottom: 0
.changes-text
	width 100%
	max-width 410px
	margin 0 auto 60px
	padding 0 0 0 110px
	background-image url(../img/check.png)
	background-repeat no-repeat
	background-position: left top
	background-size: 56px
.changes-aftertext
	text-align: center
	font-size: 20px
	font-family: "OpenSansBold", sans-serif
	span
		&:nth-of-type(1)
			font-family: "OpenSansExtraBold", sans-serif
		&:nth-of-type(2)
			background-color: #ffbf6e;
	&:last-of-type
		margin-top: 20px
#janna-talks
	padding  60px 0
.janna-talks-text
	font-size: 20px;
	font-family: 'OpenSansItalic', sans-serif
	padding 25px
	border: 7px solid #FFBF6E
	border-radius: 40px