@import '../css/_css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: "OpenSans", sans-serif
	color #2a251d
	background-color: #fff
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	border-radius: 15px
	font-size: 24px
	font-family: "RussoOne", sans-serif
	border: 0
	&:focus
		color initial
		// background-color: initial
	&:hover
		color initial
		background-color: #ffbe72

@import 'main/main'
@import 'promo/second'
@import 'promo/modal'
@import 'promo/footer'
@import 'promo/medias'